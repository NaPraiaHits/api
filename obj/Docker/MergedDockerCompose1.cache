services:
  searchapi:
    build:
      context: C:\Users\dbernardes\source\repos\SearchAPI
      dockerfile: SearchAPI/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44314'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: searchapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/SearchAPI.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 59383
      target: 80
    - published: 44314
      target: 443
    volumes:
    - C:\Users\dbernardes\source\repos\SearchAPI\SearchAPI:/app:rw
    - C:\Users\dbernardes\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\dbernardes\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\dbernardes\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\dbernardes\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'